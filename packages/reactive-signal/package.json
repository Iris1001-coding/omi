{
  "name": "reactive-signal",
  "version": "2.0.1",
  "scripts": {
    "start": "vite",
    "dev-vite": "vite",
    "build-vite": "vite build",
    "build-vite:w": "vite build --watch",
    "build": "microbundle --name ReactiveSignal && node post-build.js",
    "dev": "microbundle watch",
    "server": "live-server ./build --watch",
    "preview": "vite preview",
    "lint": "prettier --write 'src/**/*.{js,ts,tsx,jsx}' && eslint 'src/**/*.{js,ts,tsx,jsx}' 'test/**/*.{js,ts,tsx,jsx}' 'examples/**/*.{js,ts,tsx,jsx}' --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "doc": "npx typedoc --out docs src"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.23.0",
    "@babel/plugin-transform-react-jsx": "^7.22.15",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-typescript": "^7.23.0",
    "@formkit/auto-animate": "^0.8.0",
    "@types/jest": "^29.5.4",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "microbundle": "^0.15.1",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.1",
    "typescript": "^4.6.4",
    "vite": "^3.1.0"
  },
  "lint-staged": {
    "**/**.{js,ts}": [
      "eslint --fix"
    ]
  },
  "license": "MIT",
  "type": "module",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.modern.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "authors": [
    "The Omi Authors (https://github.com/Tencent/omi/contributors)"
  ],
  "repository": "Tencent/omi",
  "bugs": "https://github.com/Tencent/omi/issues/",
  "homepage": "http://omijs.org"
}
